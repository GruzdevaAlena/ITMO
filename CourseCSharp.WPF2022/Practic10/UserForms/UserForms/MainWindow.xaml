<Window x:Class="UserForms.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:my="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:local="clr-namespace:UserForms"
        mc:Ignorable="d"
        Title="MainWindow" Height="232" Width="284">
    <Grid Margin="0,0,0,0">
        <WindowsFormsHost Height="39" HorizontalAlignment="Left" Margin="30,12,0,0"
Name="windowsFormsHost1" VerticalAlignment="Top" Width="124">
            <wf:MaskedTextBox x:Name="MaskedTextBox1" Mask="(000)-000-0000"/>
        </WindowsFormsHost>
        <Button Height="23" Margin="30,76,0,0" Name="Button1" VerticalAlignment="Top" HorizontalAlignment="Left"
Click="Button1_Click" Width="120">Add to collection</Button>
        <Button Margin="30,116,0,0" Name="Button2" Height="22"
VerticalAlignment="Top" HorizontalAlignment="Left" Click="Button2_Click" Width="120">Save collection to
file</Button>
    </Grid>
</Window>
